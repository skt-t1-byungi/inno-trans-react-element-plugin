{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015"],
    "outDir": "./lib",
    "strict": true,
    "esModuleInterop": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "declaration": true
  },
  "include": [
      "./src"
  ]
}
